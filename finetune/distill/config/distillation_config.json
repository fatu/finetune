{
    "teacher_model": "Qwen/Qwen3-0.6B",
    "student_model": "Qwen/Qwen3-0.6B",
    "dataset_name": "trl-lib/tldr",
    "num_epochs": 3,
    "batch_size": 1,
    "gradient_accumulation_steps": 32,
    "learning_rate": 5e-5,
    "weight_decay": 0.01,
    "warmup_ratio": 0.1,
    "max_grad_norm": 1.0,
    "temperature": 3.0,
    "alpha": 0.7,
    "beta": 0.3,
    "save_steps": 500,
    "eval_steps": 100,
    "logging_steps": 50,
    "output_dir": "./outputs/distillation",
    "max_length": 512,
    "seed": 42,
    "torch_dtype": "bfloat16",
    "use_flash_attention": true,
    "gradient_checkpointing": true,
    "teacher_cpu_offload": false
}